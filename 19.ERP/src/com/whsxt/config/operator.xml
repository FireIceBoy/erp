<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>


	<package name="operator" namespace="/" extends="allPackage">
		<action name="operator_*" class="com.whsxt.action.OperatorAction"
			method="{1}">
			<result name="toUpdate">WEB-INF/jsp/main/pass.jsp</result>
			<result name="jump">WEB-INF/jsp/main/myJump.jsp</result>
		</action>
	</package>

</struts>

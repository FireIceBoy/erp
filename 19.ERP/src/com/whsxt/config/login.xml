<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>


	<package name="login" namespace="/" extends="allPackage">
		<action name="login_*" class="com.whsxt.action.LoginAction"
			method="{1}">
			<result name="success">/WEB-INF/jsp/main/main.jsp</result>
			<result name="error">/WEB-INF/jsp/main/login.jsp</result>
		</action>
	</package>

</struts>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	

	<package name="goods" namespace="/" extends="allPackage">
	<action name="goods_*" class="com.whsxt.action.GoodsAction" method="{1}">
	<result name="query">/WEB-INF/jsp/goods/list.jsp</result>
	<result name="toAdd">/WEB-INF/jsp/goods/add.jsp</result>
	<result name="toUpdate">/WEB-INF/jsp/goods/update.jsp</result>
	
	
	</action>
	</package>

</struts>
